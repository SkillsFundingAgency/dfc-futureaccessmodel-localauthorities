{
    "$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "appSharedResourceGroup": {
            "value": "__famSharedResourceGroup__"
        },
        "appSharedStorageAccountName": {
            "value": "__famSharedStorageAccount__"
        },
        "aseHostingEnvironmentName": {
            "value": "__FunctionAppDomain__"
        },
        "aseResourceGroup": {
            "value": "__SharedResourceGroupName__"
        },
        "cosmosDbAccountName": {
            "value": "__famSharedCosmosAccount__"
        },
        "cosmosDbResourceGroup": {
            "value": "__famSharedResourceGroup__"
        },
        "environment": {
            "value": "__ReleaseEnvironmentName__",
            "metadata": {
                "comment": "Bug in tokenisation task prevents referencing Release.EnvironmentName directly.  https://github.com/TotalALM/VSTS-Tasks/issues/29"
            }
        },
        "sharedAppServicePlanName": {
            "value": "__famSharedAppServicePlanName__"
        },
        "sharedAppServicePlanResourceGroup": {
            "value": "__famSharedResourceGroup__"
        },
        "apimResourceGroup": {
            "value": "__SharedResourceGroupName__"
        },
        "apimInstanceName": {
            "value": "__ApimServiceName__"
        },
        "apimProductId": {
            "value": "__ApimProductId__"
        },
        "apimApiName": {
            "value": "__ApimApiName__"
        },
        "apimApiSuffix": {
            "value": "__ApimApiUrlSuffix__"
        }
    }
}